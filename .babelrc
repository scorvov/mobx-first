{
  "presets": [
    "react-app",
    [
      "env",
      {
        "targets": {
          "browsers": "> 2%"
        }
      }
    ]
  ],
  "plugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-syntax-decorators",
      {
        "legacy": true
      }
    ]
  ]
}